{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "RELOAD_SW=true SW_DEV=true vite --force",
    "start-reload": "BASE_URL=/ SOURCE_MAP=true SW=true RELOAD_SW=true vite build && vite preview",
    "build": "SW=true run-p type-check build-only",
    "build-only": "vite build",
    "build-test": "npm run build && npm run test:headless",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:e2e",
    "test:headless": "npm run test:unit && npm run test:e2e:headless",
    "test:unit": "vitest run --dom",
    "test:unit:watch": "vitest --dom",
    "test:e2e": "playwright test",
    "test:e2e:headless": "npm run test:e2e:desktop && npm run test:e2e:mobile",
    "test:e2e:desktop": "playwright test --config=playwright.desktop.headless.config.ts",
    "test:e2e:mobile": "playwright test --config=playwright.mobile.headless.config.ts",
    "e2e:report": "npx playwright show-report e2e-test-results",
    "e2e:report:mobile": "npx playwright show-report e2e-mobile-test-results",
    "e2e:report:desktop": "npx playwright show-report e2e-desktop-test-results",
    "vite:inspect": "npx serve .vite-inspect",
    "rollup:inspect": "npx serve .rollup-inspect",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-pwa-assets": "pwa-assets-generator",
    "help": "node src/scripts/log-help.js"
  },
  "dependencies": {
    "@directus/sdk": "^10.3.5",
    "@vorms/core": "^1.1.0",
    "@vueuse/core": "^10.6.1",
    "idb": "^8.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.1",
    "@playwright/test": "^1.31.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.18.13",
    "@vite-pwa/assets-generator": "^0.2.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.11",
    "date-fns": "^2.30.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^9.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-inspect": "^0.7.24",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.8.22",
    "workbox-background-sync": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-window": "^7.0.0",
    "ws": "^8.13.0"
  }
}
